{
  "hosting": [
    {
      "target": "unitz-app",
      "public": "public",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "i18n": {
        "root": "/"
      },
      "rewrites": [
        {
          "source": "/courses/*",
          "function": "seo-course"
        },
        {
          "source": "/en/courses/*",
          "function": "seo-course"
        },
        {
          "source": "/vi/courses/*",
          "function": "seo-course"
        },
        {
          "source": "/page-data/courses/*/page-data.json",
          "function": "seo-coursePageData"
        },
        {
          "source": "/page-data/en/courses/*/page-data.json",
          "function": "seo-coursePageData"
        },
        {
          "source": "/page-data/vi/courses/*/page-data.json",
          "function": "seo-coursePageData"
        }
      ],
      "headers": [
        {
          "source": "/courses/*",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=2628000"
            }
          ]
        },
        {
          "source": "/en/courses/*",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=2628000"
            }
          ]
        },
        {
          "source": "/vi/courses/*",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=2628000"
            }
          ]
        },
        {
          "source": "/page-data/courses/*/page-data.json",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=2628000"
            }
          ]
        },
        {
          "source": "/page-data/en/courses/*/page-data.json",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=2628000"
            }
          ]
        },
        {
          "source": "/page-data/vi/courses/*/page-data.json",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=2628000"
            }
          ]
        },
        {
          "source": "**",
          "headers": [
            {
              "key": "Access-Control-Allow-Origin",
              "value": "*"
            }
          ]
        },
        {
          "source": "**/*",
          "headers": [
            {
              "key": "cache-control",
              "value": "public, max-age=0, must-revalidate"
            }
          ]
        },
        {
          "source": "static/**",
          "headers": [
            {
              "key": "cache-control",
              "value": "public, max-age=31536000, immutable"
            }
          ]
        },
        {
          "source": "**/*.@(css|js)",
          "headers": [
            {
              "key": "cache-control",
              "value": "public, max-age=31536000, immutable"
            }
          ]
        },
        {
          "source": "sw.js",
          "headers": [
            {
              "key": "cache-control",
              "value": "public, max-age=0, must-revalidate"
            }
          ]
        },
        {
          "source": "page-data/**",
          "headers": [
            {
              "key": "cache-control",
              "value": "public, max-age=0, must-revalidate"
            }
          ]
        }
      ]
    },
